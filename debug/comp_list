            $(document).keydown(function(e) {
                if(!args) args=[];
        
                if(e.ctrlKey || e.metaKey) isCtrl = true;
                if(e.keyCode == key.charCodeAt(0) && isCtrl) {
                    callback.apply(this, args);
                    return false;
                }
            }).keyup(function(e) {
                if(e.ctrlKey || e.metaKey) isCtrl = false;
